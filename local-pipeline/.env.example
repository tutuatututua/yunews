# Pipeline job environment (example)

# Supabase project URL, e.g. https://xxxx.supabase.co
SUPABASE_URL=

# Use service role for the pipeline since it typically needs write access.
# (Keep this in Secrets Manager / SSM in production.)
SUPABASE_SERVICE_ROLE_KEY=

# External API keys
YOUTUBE_API_KEY=
OPENAI_API_KEY=
HF_TOKEN=

#embedding model
QWEN_EMBED_MODEL="Qwen/Qwen3-Embedding-0.6B"
QWEN_EMBED_MAX_TOKENS=1024

# Optional tuning
PIPELINE_SEARCH_QUERY=stock
PIPELINE_REGION_CODE=TH
PIPELINE_LANGUAGE=en
PIPELINE_MAX_VIDEOS=10
PIPELINE_MIN_DURATION_SECONDS=60
PIPELINE_MAX_DURATION_SECONDS=2700
